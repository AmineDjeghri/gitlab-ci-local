{
  "name": "gitlab-ci-local",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "version": "4.8.0",
  "scripts": {
    "prepublishOnly": "npm run check-all && chmod +x dist/index.js",
    "pkg-linux": "pkg dist/index.js -t node14-linux-x64 -o bin/linux/gitlab-ci-local && chmod +x bin/linux/gitlab-ci-local && gzip -c bin/linux/gitlab-ci-local > bin/linux.gz",
    "pkg-macos": "pkg dist/index.js -t node14-macos-x64 -o bin/macos/gitlab-ci-local && gzip -c bin/macos/gitlab-ci-local > bin/macos.gz",
    "pkg-win": "pkg dist/index.js -t node14-win-x64 -o bin/win/gitlab-ci-local && gzip -c bin/win/gitlab-ci-local.exe > bin/win.gz",
    "pkg-all": "npm run pkg-linux && npm run pkg-macos && npm run pkg-win",
    "pkg": "pkg",
    "build": "tsc",
    "check-all": "npm run build && npm run --silent lint && npm run ncu && npm audit --parseable",
    "lint": "tslint --project .",
    "lint-fix": "tslint --fix --project .",
    "ncu": "ncu --target latest -e 2",
    "test-yml-spec": "node -r source-map-support/register dist/index.js --manual manual_job_invoked_from_cli --cwd tests/test-yml-spec",
    "test-exec-never-job": "node -r source-map-support/register dist/index.js never_job --cwd tests/test-yml-spec",
    "start": "node -r source-map-support/register dist/index.js"
  },
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "clone": "^2.1.2",
    "dateformat": "^4.4.1",
    "deep-extend": "^0.6.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "pretty-hrtime": "^1.0.3",
    "js-yaml": "^4.0.0",
    "yargs": "^16.2.0"
  },
  "pkg": {
    "targets": [
      "node14"
    ]
  },
  "author": "Mads Jon Nielsen <madsjon@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/clone": "^2.1.0",
    "@types/deep-extend": "^0.4.31",
    "@types/fs-extra": "^9.0.6",
    "@types/js-yaml": "^4.0.0",
    "@types/pretty-hrtime": "^1.0.0",
    "@types/shelljs": "^0.8.8",
    "@types/yaml": "^1.9.7",
    "@types/yargs": "^15.0.12",
    "npm-check-updates": "^10.2.5",
    "pkg": "^4.4.9",
    "source-map-support": "^0.5.19",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
